nIn/signIn.component.jsx",[],["256","257"],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/form-input/form-input.component.jsx",[],["258","259"],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/custom-button/custom-button-component.jsx",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/fireBase/firebase.utils.js",["260"],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/signUp/signUp.component.jsx",["261","262","263","264","265"],"import React from 'react';\n//import './signUp.style.scss';\nimport {auth, createUserProfileDocument} from '../../fireBase/firebase.utils';\nimport FormInput from '../form-input/form-input.component';\nimport CustomButton from '../custom-button/custom-button-component';\nimport {SignUpContainer,TitleContainer} from './signUp.style';\nclass SignUp extends React.Component\n{\n    constructor(props)\n    {\n        super(props);\n        this.state={\n            'displayName':\"\",\n            'email':\"\",\n            'password':\"\",\n            'confirmPassword':\"\"\n        }\n        \n    }\n\n    handleSubmit = async event=>{\n        event.preventDefault();\n        const { displayName, email, password, confirmPassword } = this.state;\n         \n        if(password!=confirmPassword){\n            alert ('Password and Confirm Password does not match');\n            return;\n        }\n        try{\n            const {user}= await auth.createUserWithEmailAndPassword(email,password);\n            //console.log(displayName+\" on signup component\")\n            //console.log(user)\n            await createUserProfileDocument(user, {displayName});\n            this.setState({\n                'displayName':\"\",\n                'email':\"\",\n                'password':\"\",\n                'confirmPassword':\"\",\n            });\n        }\n        catch(error){\n            console.error(error);\n        }\n    }\n\n    handleChange=event=>{\n        const {name,value}=event.target;\n        this.setState({[name]:value});\n    }\n\n    \n\n    render()\n    {\n        const { displayName, email, password, confirmPassword } = this.state;\n        return(\n            <SignUpContainer>\n                <TitleContainer className='title'>I do not hava an Account!</TitleContainer>\n                <span>Sign Up with your Email Id and password</span>\n                <form className='sign-up-form' onSubmit={this.handleSubmit}>\n                    <FormInput\n                        handleChange={this.handleChange}\n                        label='Display Name'\n                        type='text'\n                        name='displayName'\n                        className='displayName'\n                        value={this.state.displayName}\n                        required\n                    />\n                    <FormInput\n                        type='email'\n                        name='email'\n                        className='email'\n                        value={this.state.email}\n                        handleChange={this.handleChange}\n                        label='Email'\n                        required\n                    />\n                    <FormInput\n                        type='password'\n                        name='password'\n                        className='password'\n                        value={this.state.password}\n                        handleChange={this.handleChange}\n                        label='Password'\n                        required\n                    />\n                    <FormInput\n                        type='password'\n                        name='confirmPassword'\n                        className='confirmPassword'\n                        value={this.state.confirmPassword}\n                        handleChange={this.handleChange}\n                        label='Password'\n                        required\n                    />\n                    \n                    <CustomButton type='submit'>SIGN UP</CustomButton>\n                </form>\n            </SignUpContainer>\n        );\n    }\n}\nexport default SignUp; ","/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/user/user.reducer.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/root-reducer.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/store.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/user/user.actions.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/user/user.type.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/cartIcon/cartIcon.component.jsx",["266","267"],"import React from 'react';\nimport {createStructuredSelector} from 'reselect';\nimport {connect} from 'react-redux';\nimport {toggleCartHidden} from '../../redux/cart/cart.actions'\nimport {selectCartItemCount, selectCartItems} from '../../redux/cart/cart.selector';\n\nimport {ReactComponent as ShoppingIcon} from '../../assets/shopping-bag.svg';\n//import './cartIcon.style.scss';\nimport {CartIconContainer,\n    ShoppingIconContainer,\n    ItemCountContainer\n} from './cartIcon.style';\n\nconst CartIcon =({toggleCartHidden,itemCount})=>{\n   \n    return(\n    <CartIconContainer onClick={toggleCartHidden}>\n        <ShoppingIconContainer/>\n        <ItemCountContainer>{itemCount}</ItemCountContainer>\n    </CartIconContainer>\n)};\n\nconst mapDispatchToprops= dispatch=>({\n    toggleCartHidden: ()=>dispatch(toggleCartHidden())\n})\n\nconst mapStateToProps=createStructuredSelector(\n    {\n     itemCount:selectCartItemCount\n\n})\nexport default connect(mapStateToProps, mapDispatchToprops)(CartIcon);","/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/cartDropDown/cartDropDown.component.jsx",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/cart/cart.actions.js",[],["268","269"],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/cart/cart.type.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/cart/cart.redux.js",["270","271"],"import CartActionType from './cart.type';\nimport {updateCartWithItems,removeAndUpdateCart} from './cartUtils';\n\nconst INITIAL_STATE={\n    hidden:true,\n    cartItems:[],\n}\n\nconst cartReducer=(state=INITIAL_STATE,action)=>{\n    switch(action.type){\n        case CartActionType.TOGGLE_CART_HIDDEN:\n            return{\n                ...state,\n                hidden: !state.hidden,\n            };\n        case CartActionType.ADD_ITEM_TO_CART:\n            return{\n                ...state,\n                cartItems:updateCartWithItems(state.cartItems,action.payload),\n            };\n        case CartActionType.CLEAR_ITEM_FROM_CART:\n            return{\n                ...state,\n                cartItems:state.cartItems.filter(cartItem=>(cartItem.name!=action.payload.name)||(cartItem.id!=action.payload.id)),\n            }\n        case CartActionType.REMOVE_ITEM_FROM_CART:\n            return{\n                ...state,\n                cartItems:removeAndUpdateCart(state.cartItems,action.payload),\n            }    \n        default:\n            return state;\n\n    }\n}\nexport default cartReducer;",["272","273"],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/cart/cartUtils.js",["274","275","276","277"],"import CartItem from \"../../components/cart-item/cart-item.component\";\n\nexport const updateCartWithItems=(cartItems,item)=>{\n    //console.log(item);\n    const existingItem=cartItems.find(cartItem=>((cartItem.id===item.id)&&(cartItem.name===item.name)));\n    if(existingItem){\n        return cartItems.map(cartItem=>\n            (cartItem.id===item.id && cartItem.name===item.name)?\n            {...cartItem, quantity:cartItem.quantity+1}:\n            {...cartItem}        \n        )}\n    //console.log(item);\n    return [...cartItems, {...item,quantity:1}]\n};\n\nexport const removeAndUpdateCart=(cartItems,item)=>{\n    const itemExist=cartItems.find(cartItem=>((cartItem.id===item.id)&&(cartItem.name===item.name)));\n    if(itemExist.quantity==1){\n        return cartItems.filter(cartItem=>((cartItem.id!=item.id)||(cartItem.name!=item.name)));\n    }\n    return cartItems.map(cartItem=>\n        (cartItem.id===item.id && cartItem.name===item.name)?\n        {...cartItem,quantity:cartItem.quantity-1}:\n        {...cartItem}\n    )\n}\n","/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/cart-item/cart-item.component.jsx",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/cart/cart.selector.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/pages/checkOutPage/checkoutpage.component.jsx",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/redux/user/user.selector.js",[],"/home/rupamswain1/Documents/ReactApps/react-ecommerce-app/src/components/checkOutItem/checkOutItem.component.jsx",["278"],"import React from 'react';\nimport './checkOutItem.style.scss';\nimport {connect} from 'react-redux';\nimport {clearItemFromCart,addItemsToCart,removeItemFromCart} from '../../redux/cart/cart.actions'\nimport CartItem from '../cart-item/cart-item.component';\n\nconst CheckOutItem=({cartItem,clearItem,addItem,removeItem})=>\n{ \n    const {name,imageUrl,price,quantity}=cartItem;\n\n    return(\n    <div className='checkout-item'>\n        <div className='image-container'>\n            <img src={imageUrl} alt='item'/>\n        </div>\n        <span className='name'>{name}</span>\n        <span className='quantity'>\n            <div className=\"arrow\" onClick={()=>removeItem(cartItem)}>&#10092;</div>\n            <span className=\"value\">{quantity}</span>    \n            <div className=\"arrow\" onClick={()=>addItem(cartItem)}>&#10093;</div>\n        </span>\n        <span className='price'>{price}</span>\n        <div class